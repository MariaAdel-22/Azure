<h1 style="color:red">
    Upload Azure File
</h1>

<a asp-controller="AzureFiles"
   asp-action="Index">
    Volver a Index
</a>

<form method="post" enctype="multipart/form-data">
    <input type="file" name="file" class="form-control" />
    <button type="submit" class="btn btn-info">
        Upload File
    </button>
</form>
<h2>@ViewData["MENSAJE"]</h2>

